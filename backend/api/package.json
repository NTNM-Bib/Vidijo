{
  "name": "vidijo-api",
  "version": "1.1.0",
  "description": "The API of Vidijo. Requests data from other vidijo services and aggregates them for end users.",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "prod": "tsc && cross-env NODE_ENV=production npm run forever",
    "dev": "tsc && concurrently \"npm run build-and-watch\" \"nodemon --legacy-watch dist/src/index.js\"",
    "docker": "tsc && gulp && node dist/src/index.js",
    "build-and-watch": "gulp && concurrently \"tsc -w\" \"gulp watch\"",
    "forever": "forever start dist/src/index.js",
    "test": "mocha -r ts-node/register test/default.test.ts"
  },
